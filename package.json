{
  "name": "scot4",
  "version": "0.0.0-development",
  "description": "Development environment of packages",
  "scripts": {
    "start": "babel-node src/cli/index.js",
    "clean": "rimraf lib/*",
    "precompile": "npm run clean",
    "compile": "babel src --out-dir lib --source-maps inline --copy-files --ignore cli/**/templates/*",
    "postcompile": "rsync --include='*/' --include='.babelrc' --include='cli/**/templates/***' --exclude='*' -a src/ lib/",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "main": "lib/scot4.js",
  "bin": {
    "scot4": "bin/scot4.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Travelab/scot4.git"
  },
  "author": "Dmitry Bukov <sukazavr@gmail.com> (sukazavr.ru)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Travelab/scot4/issues"
  },
  "homepage": "https://github.com/Travelab/scot4",
  "dependencies": {
    "@kadira/storybook": "^2.13.0",
    "@webpack-blocks/babel6": "^0.3.0",
